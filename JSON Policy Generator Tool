			function createNode(element) {
			  return document.createElement(element);
			}

			function append(parent, el) {
			  return parent.appendChild(el);
			}

			function generateErmConsoleScript() {
		  var ec2ErmInput = document.getElementById("ec2ErmCheck").value;
		  	if (ec2ErmCheck.checked == true){
		    ec2ErmString = ec2ErmJSON;
		    } else {
				    ec2ErmString = "";
				};
		  var rdsErmInput = document.getElementById("rdsErmCheck").value;
		  	if (rdsErmCheck.checked == true){
		    rdsErmString = rdsErmJSON;
		    } else {
				    rdsErmString = "";
				};
		  var rsErmInput = document.getElementById("rsErmCheck").value;
		  	if (rsErmCheck.checked == true){
		    rsErmString = rsErmJSON;
		    } else {
				    rsErmString = "";
				};
		  var ecErmInput = document.getElementById("ecErmCheck").value;
		  	if (ecErmCheck.checked == true){
		    ecErmString = ecErmJSON;
		    } else {
				    ecErmString = "";
				};
		  var azureErmInput = document.getElementById("azureErmCheck").value;
		  	if (azureErmCheck.checked == true){
		    azureErmString = azureErmJSON;
		    } else {
				    azureErmString = "";
				};
		  var mergedErmPayload = ermPayload.replace(/<ermClouds>/gi,ec2ErmString + rdsErmString + rsErmString + ecErmString + azureErmString);
		  var generatedErmConsoleScript = ermConsoleScript
		  	.replace(/<ermPayload>/gi,mergedErmPayload);
			document.getElementById("ermConsoleScriptTextBox").value = generatedErmConsoleScript;
		}

			function copyErmConsoleScript() {
			  var copyErmConsoleText = document.getElementById("ermConsoleScriptTextBox");
				copyErmConsoleText.select();
				document.execCommand("copy");
			  };
		</script>
	</div>

