			function createNode(element) {
			  return document.createElement(element);
			}

			function append(parent, el) {
			  return parent.appendChild(el);
			}

			function generateWsConsoleScript() {
		  var awsWsInput = document.getElementById("awsWsCheck").value;
		  	if (awsWsCheck.checked == true){
		    awsWsString = awsWsJSON;
		    } else {
				    awsWsString = "";
				};
		  var azureWsInput = document.getElementById("azureWsCheck").value;
		  	if (azureWsCheck.checked == true){
		    azureWsString = azureWsJSON;
		    } else {
				    azureWsString = "";
				};
		  var gcpWsInput = document.getElementById("gcpWsCheck").value;
		  	if (gcpWsCheck.checked == true){
		    gcpWsString = gcpWsJSON;
		    } else {
				    gcpWsString = "";
				};
		  var mergedWsPayload = wsPayload.replace(/<wsClouds>/gi,awsWsString + azureWsString + gcpWsString);
		  var generatedWsConsoleScript = wsConsoleScript
		  	.replace(/<wsPayload>/gi,mergedWsPayload);
			document.getElementById("wsConsoleScriptTextBox").value = generatedWsConsoleScript;
		}

			function copyWsConsoleScript() {
			  var copyWsConsoleText = document.getElementById("wsConsoleScriptTextBox");
				copyWsConsoleText.select();
				document.execCommand("copy");
			  };
		</script>
