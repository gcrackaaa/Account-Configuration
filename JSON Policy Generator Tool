			function generateXxxConsoleScript() {
		  var awsXxxInput = document.getElementById("awsXxxCheck").value;
		  	if (awsXxxCheck.checked == true){
		    awsXxxString = awsXxxJSON;
		    } else {
				    awsXxxString = "";
				};
		  var azureXxxInput = document.getElementById("azureXxxCheck").value;
		  	if (azureXxxCheck.checked == true){
		    azureXxxString = azureXxxJSON;
		    } else {
				    azureXxxString = "";
				};
		  var gcpXxxInput = document.getElementById("gcpXxxCheck").value;
		  	if (gcpXxxCheck.checked == true){
		    gcpXxxString = gcpXxxJSON;
		    } else {
				    gcpXxxString = "";
				};
		  var mergedXxxPayload = xxxPayload.replace(/<xxxClouds>/gi,awsXxxString + azureXxxString + gcpXxxString);
		  var generatedXxxConsoleScript = xxxConsoleScript
		  	.replace(/<xxxPayload>/gi,mergedXxxPayload);
			document.getElementById("xxxConsoleScriptTextBox").value = generatedXxxConsoleScript;
		}

			function copyXxxConsoleScript() {
			  var copyXxxConsoleText = document.getElementById("xxxConsoleScriptTextBox");
				copyXxxConsoleText.select();
				document.execCommand("copy");
			  };
