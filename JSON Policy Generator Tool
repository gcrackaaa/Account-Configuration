			function generateRsConsoleScript() {
			  var awsMaxRsInput = document.getElementById("awsMaxRsCheck").value;
			  	if (awsMaxRsCheck.checked == true){
			    awsMaxRsString = awsMaxRsJSON;
			    } else {
					    awsMaxRsString = "";
					};
			  var awsAvgRsInput = document.getElementById("awsAvgRsCheck").value;
			  	if (awsAvgRsCheck.checked == true){
			    awsAvgRsString = awsAvgRsJSON;
			    } else {
					    awsAvgRsString = "";
					};
			  var azureMaxRsInput = document.getElementById("azureMaxRsCheck").value;
			  	if (azureMaxRsCheck.checked == true){
			    azureMaxRsString = azureMaxRsJSON;
			    } else {
					    azureMaxRsString = "";
					};
			  var azureAvgRsInput = document.getElementById("azureAvgRsCheck").value;
			  	if (azureAvgRsCheck.checked == true){
			    azureAvgRsString = azureAvgRsJSON;
			    } else {
					    azureAvgRsString = "";
					};
			  var dcMaxRsInput = document.getElementById("dcMaxRsCheck").value;
			  	if (dcMaxRsCheck.checked == true){
			    dcMaxRsString = dcMaxRsJSON;
			    } else {
					    dcMaxRsString = "";
					};
			  var maxVolumeRsInput = document.getElementById("maxVolumeRsCheck").value;
			  	if (maxVolumeRsCheck.checked == true){
			    maxVolumeRsString = maxVolumeRsJSON;
			    } else {
					    maxVolumeRsString = "";
					};

			  	document.getElementById("rsConsoleScriptTextBox").value = awsMaxRsString + awsAvgRsString + azureMaxRsString + azureAvgRsString + dcMaxRsString + maxVolumeRsString;
				}

			function copyRsConsoleScript() {
			  var copyRsConsoleText = document.getElementById("rsConsoleScriptTextBox");
				copyRsConsoleText.select();
				document.execCommand("copy");
			};
		</script>
	</div>

