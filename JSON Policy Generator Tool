	<script>
		function showModule(elem) {
			if(elem.selectedIndex != 0) {
		//hide the divs
			for(var i=0; i < divsO.length; i++) {
				divsO[i].style.display = 'none';
			}
		//unhide the selected div
		document.getElementById('div'+elem.value).style.display = 'block';
			}
		}

		window.onload=function() {
		//get the divs to show/hide
			divsO = document.getElementById("scriptTypeForm").getElementsByTagName('div');
		}
	</script>
		<form action="#" method="post" id="scriptTypeForm">
		<select id="scriptType" onchange="showModule(this)" style="margin: 20px">
			<option value="">-- Select --</option>
			<option value="1">Rightsizing</option>
            <option value="2">Tagging</option>
            <option value="3">Wasted Spend</option>
            <option value="4">Expiring Reservations/Modifications</option>
            <option value="5">Account Status/Changes</option>
            <option value="6">Underutilized Resources</option>
            <option value="7">Cost Increase</option>
            <option value="8">Over Budget</option>
		</select>

