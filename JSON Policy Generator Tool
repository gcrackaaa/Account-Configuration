			function append(parent, el) {
			  return parent.appendChild(el);
			}

			function generateAscConsoleScript() {
		  var awsAscInput = document.getElementById("awsAscCheck").value;
		  	if (awsAscCheck.checked == true){
		    awsAscString = awsAscJSON;
		    } else {
				    awsAscString = "";
				};
		  var azureAscInput = document.getElementById("azureAscCheck").value;
		  	if (azureAscCheck.checked == true){
		    azureAscString = azureAscJSON;
		    } else {
				    azureAscString = "";
				};
		  var gcpAscInput = document.getElementById("gcpAscCheck").value;
		  	if (gcpAscCheck.checked == true){
		    gcpAscString = gcpAscJSON;
		    } else {
				    gcpAscString = "";
				};
		  var dcAscInput = document.getElementById("dcAscCheck").value;
		  	if (dcAscCheck.checked == true){
		    dcAscString = dcAscJSON;
		    } else {
				    dcAscString = "";
				};
		  var chAscInput = document.getElementById("chAscCheck").value;
		  	if (chAscCheck.checked == true){
		    chAscString = chAscJSON;
		    } else {
				    chAscString = "";
				};
		  var agentAscInput = document.getElementById("agentAscCheck").value;
		  	if (agentAscCheck.checked == true){
		    agentAscString = agentAscJSON;
		    } else {
				    agentAscString = "";
				};
		  var mergedAscPayload = ascPayload.replace(/<ascClouds>/gi,awsAscString + azureAscString + gcpAscString + dcAscString + chAscString + agentAscString);
		  var generatedAscConsoleScript = ascConsoleScript
		  	.replace(/<ascPayload>/gi,mergedAscPayload);
			document.getElementById("ascConsoleScriptTextBox").value = generatedAscConsoleScript;
		}

			function copyAscConsoleScript() {
			  var copyAscConsoleText = document.getElementById("ascConsoleScriptTextBox");
				copyAscConsoleText.select();
				document.execCommand("copy");
			  };
		</script>
