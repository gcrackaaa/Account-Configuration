			const awsTagJSON = '{"name": "Untagged Asset","catchall": false,"enabled": true,"evaluation_interval": {"repeat": "daily","repeat_on_day": 1,"repeat_weekdays": [1],"repeat_time": "6:00 AM -0500","repeat_hour_interval": 4},"documentation": "","conditions": [],"filters": [],"groups": [],"scope": {"name": null,"label": "<none>"},"rule_groups": [{"name": "Rule Group 1","rules": [{"name": "Rule 1","enabled": true,"conditions": [{"enabled": true,"measure": {"id": "instant_aws_assets_tag_policy","label": "Tags","source": "instant","cube_id": null,"measure_id": null,"unit": "","periods": ["instant"],"exclude_filters": null,"translated_description": null,"functions": null,"start_time": null},"topic": {"id": "configuration","label": "Configuration","policy_type": null},"granularity": "instant","value": 1,"time": "is","abs": "abs","direction": "less","days": 1,"tag_conditions": [{"condition_type": "empty"}],"runlist_conditions": [],"password_policy": {},"filters": []}],"level": 10,"actions": []}]}],"resource_type": "aws_assets","clear_suppressions": false} '
			const azureTagJSON = 'Azure JSON'
			const gcpTagJSON = 'GCP JSON '
			const dcTagJSON = 'DC JSON '

			function createNode(element) {
			  return document.createElement(element);
			}

			function append(parent, el) {
			  return parent.appendChild(el);
			}

			function generateTagConsoleScript() {
			  var awsTagInput = document.getElementById("awsTagCheck").value;
			  	if (awsTagCheck.checked == true){
			    awsTagString = awsTagJSON;
			    } else {
					    awsTagString = "";
					};
			  var azureTagInput = document.getElementById("azureTagCheck").value;
			  	if (azureTagCheck.checked == true){
			    azureTagString = azureTagJSON;
			    } else {
					    azureTagString = "";
					};
			  var gcpTagInput = document.getElementById("gcpTagCheck").value;
			  	if (gcpTagCheck.checked == true){
			    gcpTagString = gcpTagJSON;
			    } else {
					    gcpTagString = "";
					};
			  var dcTagInput = document.getElementById("dcTagCheck").value;
			  	if (dcTagCheck.checked == true){
			    dcTagString = dcTagJSON;
			    } else {
					    dcTagString = "";
					};
			  var mergedTagPayload = tagPayload.replace(/<tagClouds>/gi,awsTagString + azureTagString + gcpTagString + dcTagString);
			  var generatedTagConsoleScript = tagConsoleScript
			  	.replace(/<tagPayload>/gi,mergedTagPayload);
				document.getElementById("tagConsoleScriptTextBox").value = generatedTagConsoleScript;
			}

			function copyTagConsoleScript() {
			  var copyTagConsoleText = document.getElementById("tagConsoleScriptTextBox");
				copyTagConsoleText.select();
				document.execCommand("copy");
			  };
		</script>
	</div>
	
